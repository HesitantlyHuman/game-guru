{
  "compilerOptions": {
    "outDir": "./dist/",
    "lib": [
      "es2015",
      "dom"
    ],
    "target": "es5",
    "sourceMap": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
  }
}